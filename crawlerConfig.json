{
    "appId": "O66TCPOEXP",
    "apiKey": "943184a59fb1852faddd7c025bbb4320",
    // 填写自己的索引名称
    "index_name": "HKBlog",
    // 填写自己的网站地址
    "start_urls": [
        "https://hk-hub.github.io/"
    ],
    "rateLimit": 8,
    "maxDepth": 10,
    "selectors": {
        "lvl0": {
            "selector": "",
            "defaultValue": "Documentation"
        },
        "lvl1": ".content h1",
        "lvl2": ".content h2",
        "lvl3": ".content h3",
        "lvl4": ".content h4",
        "lvl5": ".content h5",
        "content": ".content p, .content li",
        "lang": {
            "selector": "/html/@lang",
            "type": "xpath",
            "global": true
        }
    },
    "selectors_exclude": [
        "aside",
        ".page-footer",
        ".next-and-prev-link",
        ".table-of-contents"
    ],
    "custom_settings": {
        "attributesForFaceting": [
            "lang",
            "tags"
        ]
    },
    "js_render": true
}